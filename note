SELECT COUNT(status), renewal FROM registrasi WHERE status = 'supporter' GROUP by renewal

SELECT COUNT(status) FROM registrasi WHERE renewal IS NULL AND status = 'supporter'

SELECT COUNT(registrasi.id), YEAR(tanggal), MONTH(tanggal) FROM registrasi WHERE registrasi.id = (SELECT registrasi.id FROM registrasi JOIN biodata ON id_bio = biodata.id ORDER BY registrasi.id DESC LIMIT 1) GROUP by YEAR(tanggal), MONTH(tanggal)


